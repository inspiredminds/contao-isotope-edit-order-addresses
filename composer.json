{
    "name": "inspiredminds/contao-isotope-edit-order-addresses",
    "description": "Allows to edit the billing and shipping address of a Contao Isotope order.",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/inspiredminds/contao-isotope-edit-order-addresses",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
	"support": {
		"issues": "https://github.com/inspiredminds/contao-isotope-edit-order-addresses/issues",
		"source": "https://github.com/inspiredminds/contao-isotope-edit-order-addresses",
		"forum": "https://community.contao.org/de"
	},
	"funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "contao/core-bundle": "^4.9",
        "isotope/isotope-core": "^2.6",
        "symfony/config": "^4.4 || ^5.2",
        "symfony/dependency-injection": "^4.4 || ^5.2",
        "symfony/http-foundation": "^4.4 || ^5.2",
        "symfony/http-kernel": "^4.4 || ^5.2"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoIsotopeEditOrderAddresses\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoIsotopeEditOrderAddresses\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    }
}
